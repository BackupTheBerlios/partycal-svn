define('PartyCal_MAIN_METHOD', 'PartyCalAdmin::main()');

require_once  dirname(__FILE__).'/partycal';

if (PartyCal_MAIN_METHOD == 'PartyCalAdmin::main()') {
	$PartyCalAdmin = new PartyCalAdmin( 'partycal-cli-admin', $argv );
	$PartyCalAdmin->main();
}
